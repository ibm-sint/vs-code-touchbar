{
	"name": "vs-code-touchbar",
	"publisher": "Maruan-Alush",
	"displayName": "vs-code-touchbar",
	"description": "",
	"icon": "./assets/icon.png",
	"repository": {
		"url": "https://github.ibm.com/Maruan-Alush/vs-code-touchbar"
	},
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.NPM_run",
				"title": "Run",
				"icon": "./assets/rightArrow.png"
			},
			{
				"command": "extension.NPM_build",
				"title": "Build"
			}
		],
		"menus": {
			"touchBar": [
				{
					"command": "extension.NPM_run",
					"group": "editing"
				},
				{
					"command": "extension.NPM_build",
					"group": "editing"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
